    "@vue/cli-plugin-eslint": "4.5.6",
    "@vue/cli-service": "4.5.6",
    "babel-eslint": "10.1.0",
    "eslint": "7.10.0",
    "sass": "1.26.12",
    "sass-loader": "10.0.2",
    "vue-cli-plugin-vuetify": "2.0.7",
    "vue-template-compiler": "2.6.12",
    "vuetify-loader": "1.6.0"
  },
  "resolutions": {
